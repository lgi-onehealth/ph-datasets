manifest {
    name = "ph-datasets"
    version = "0.1.0"
    author = "Anders Goncalves da Silva"
}

executor {
    // required to avoid getting errors from Entrez API about 
    // overloading the server
    submitRateLimit = '1sec'
}

includeConfig './confs/data.config'
includeConfig './confs/params.config'
includeConfig './confs/modules.config'